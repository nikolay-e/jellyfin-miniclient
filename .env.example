# Jellyfin API Configuration
# Copy this file to .env and fill in your values

# Jellyfin API Key / Access Token (required)
# Get this from your Jellyfin server: Dashboard > API Keys
JELLYFIN_API_KEY=your-api-key-here

# Jellyfin Server URL (optional, can be set at runtime)
# Example: https://jellyfin.example.com or http://localhost:8096
JELLYFIN_SERVER_URL=https://your-server.com

# Client Configuration (optional)
JELLYFIN_CLIENT_NAME=Jellyfin Mini Client
JELLYFIN_DEVICE_NAME=Web Browser
JELLYFIN_DEVICE_ID=
